{
  "info": {
    "name": "EMNA Recruitment System API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "http://localhost:8000/api/auth/login/",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8000",
          "path": [
            "api",
            "auth",
            "login",
            ""
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"username\": \"admin\", \"password\": \"yourpassword\"}"
        }
      }
    },
    {
      "name": "Dashboard Summary",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{access}}"
          }
        ],
        "url": {
          "raw": "http://localhost:8000/api/dashboard/summary/",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "8000",
          "path": [
            "api",
            "dashboard",
            "summary",
            ""
          ]
        }
      }
    }
  ]
}